<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical"
    android:gravity="center" 
    tools:context="inacaptemuco.cl.asegurateseguros.ResultadoActivity">
<TextView
        android:id="@+id/txtv_patente"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="24sp"
        />
    <TextView
        android:id="@+id/txtv_marca"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="24sp"
        />
    <TextView
        android:id="@+id/txtv_modelo"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="24sp"
        />
    <TextView
        android:id="@+id/txtv_ano"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="24sp"
        />
    <TextView
        android:id="@+id/txtv_ufActual"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="24sp"
        />
    <TextView
        android:id="@+id/txtv_valor"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="24sp"
        />
    <TextView
        android:id="@+id/txtv_resultado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textSize="24sp"
        />


















    <ImageView
        android:id="@+id/imv_resultado"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content" />
</LinearLayout>
